{
  "name": "nextjs",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@material-ui/core": "latest",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/styles": "latest",
    "apollo-boost": "^0.4.4",
    "clsx": "latest",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "next": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3000 cy:run"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/cypress": "^4.1.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.0.2",
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "cypress": "^3.4.1",
    "jest": "^24.8.0",
    "prettier": "1.18.2",
    "start-server-and-test": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,ts,tsx}",
      "!**/next.config.js",
      "!**/theme.ts",
      "!**/pages/**",
      "!**/cypress/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
